<% if (!userdata) {%>
    <script>
        alert("관리자로 로그인하고 사용할 수 있습니다.")
        location.href = "/admin";
    </script>
<% } %>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>admin mgz add</title>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@48,400,0,0" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Nanum+Myeongjo:wght@400;700;800&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
    <%- include ("common/admin_header.ejs")%>
    <div id="container">
        <ul class="admin_menu">
            <li><a href="/admin/prdlist">상품등록페이지</a></li>
            <li><a href="/admin/storelist">매장등록페이지</a></li>
            <li><a href="/admin/mgzlist">매거진등록페이지</a></li>
        </ul>
        <ul class="prdWrap">
            <% for (let i = 0; i < mgzData.length ;i++ ) {%>
                <div class="list">
                    <img src="/upload/<%- mgzData[i].thumbnail%>">
                    <img src="/upload/<%- mgzData[i].contImg%>">
                    <h3>Num.<%- mgzData[i].num%></h3>
                    <p>Subtitle:<%- mgzData[i].subtitle%></p>
                    <p>Title:<%- mgzData[i].title%></p>
                    <p>Content:<%- mgzData[i].content%></p>
                    <p>Content1:<%- mgzData[i].content1%></p>
                </div>
            <% } %>
        </ul>
        <form action="/add/mgzlist" method="post" enctype="multipart/form-data">
            <input type="text" name="subtitle" placeholder="subtitle">
            <input type="text" name="title"  placeholder="title">
            <input type="text" name="content" placeholder="content">
            <input type="text" name="content1" placeholder="content1">
            <input type="file" name="thumbnail" >
            <input type="file" name="thumbnail" >
            <button type="submit">submit</button>
        </form>
    </div>
   
</body>
</html>